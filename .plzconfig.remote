[remote]
url = localhost:7772
instance = mettle
secure = false
numexecutors = 10
displayurl = http://localhost:7779
tokenfile = grpcutil/token.txt
cacheduration = 10m

[build]
hashfunction = sha256

[please]
location = ~/.please
